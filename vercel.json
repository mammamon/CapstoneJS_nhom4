{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/product.html"
      }
    ],
    "headers": [
      {
        "source": "/asset/css/.*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css"
          }
        ]
      },
      {
        "source": "/asset/js/.*",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      }
    ]
  }
  